{
  "name": "aether.link",
  "version": "0.1.7",
  "author": "Smultar <Jason@smultar.com>",
  "description": "Quality of life tool, for Final Fantasy modders.",
  "private": false,
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@react-three/drei": "^7.24.0",
    "@react-three/fiber": "^7.0.27",
    "adm-zip": "^0.5.9",
    "archiver": "^5.3.1",
    "cross-env": "^7.0.3",
    "electron-acrylic-window": "^0.5.9",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.0",
    "react-three-fiber": "^6.0.13",
    "sass": "^1.49.11",
    "three": "^0.135.0",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "ansi-html": ">=0.0.8",
    "async": ">=2.6.4",
    "nth-check": ">=2.0.1",
    "concurrently": "^6.3.0",
    "electron": "^15.3.5",
    "electron-builder": "^22.5.1",
    "electronmon": "^2.0.2"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm run react:start\" \"wait-on http://localhost:3000 && electronmon --trace-warnings .\"",
    "electron:package:win": "\"npm react:build \" && \"electron-builder -w --publish never\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.smultar.aether.link",
    "productName": "aether.link",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "smultar",
        "repo": "aether.link"
      }
    ],
    "portable": {
      "artifactName": "aether.link.exe"
    },
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": "deb"
    }
  },
  "keywords": [
    "ffxiv",
    "ffxiv-modding",
    "ffxiv-modding-tools",
    "ffxiv-modding-tools-aether.link",
    "ffxiv-modding-tools-aether.link-smultar",
    "final-fantasy-xiv",
    "modding-tools",
    "smultar",
    "aether.link"
  ],
  "repository": {
    "private": false,
    "owner": "smultar",
    "name": "aether.link",
    "description": "Quality of life tool, for Final Fantasy modders.",
    "url": "https://github.com/smultar/aether.link.git",
    "type": "git"
  }
}
